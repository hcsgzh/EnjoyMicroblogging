<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/homeView">
    <com.aisidi.kuaiyue.component.MyListView
        android:id="@+id/home_list"
        android:dividerHeight="0dp"
        android:divider="@color/alpha"
        android:fadingEdge="none"
        android:fadingEdgeLength="0dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:listSelector="@color/alpha"
        android:cacheColorHint="#00000000"
        android:layout_width="match_parent"
        android:layout_height="fill_parent" >
    </com.aisidi.kuaiyue.component.MyListView>
    <com.aisidi.kuaiyue.component.CustomerBar
		    android:id="@+id/waiting_bar"
		    android:layout_gravity="center"
		    android:layout_width="@dimen/bar"
			android:layout_height="@dimen/bar_h"/>
</FrameLayout>